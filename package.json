{
  "name": "overwatch-league-fantasy",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next src/app",
    "sort": "sort-package-json"
  },
  "dependencies": {
    "firebase-admin": "^5.8.0",
    "firebase-functions": "^0.8.0",
    "next": "^4.2.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "babel-eslint": "^8.2.1",
    "cpx": "^1.5.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "firebase-tools": "^3.16.0",
    "flow-bin": "^0.63.1",
    "flow-coverage-report": "^0.4.0",
    "flow-typed": "^2.2.3",
    "jest": "^22.0.6",
    "rimraf": "^2.6.2",
    "sort-package-json": "^1.7.1"
  },
  "flow-coverage-report": {
    "concurrentFiles": 5,
    "excludeGlob": [
      "src/**/*.spec.js"
    ],
    "threshold": 80
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "notify": true
  }
}
